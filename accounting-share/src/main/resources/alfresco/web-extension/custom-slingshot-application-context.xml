<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
   <!-- Add in the custom action messages -->
   <bean id="com.ixxus.fayes.webscripts.resources" class="org.alfresco.i18n.ResourceBundleBootstrapComponent">
      <property name="resourceBundles">
         <list>
            <value>alfresco.web-extension.messages.sample</value>
         </list>
      </property>
   </bean>

   <bean id="ixxus.sample.evaluator" class="com.ixxus.fayes.SampleActionEvaluator" />

</beans>